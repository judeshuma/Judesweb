/* ------------------ Homepage CSS starts here -------------- */
.home {
    background-color: black;
}
body {
    margin: 0;
    padding: 0;  
    background-size: cover;
    background-repeat: no-repeat;
    background-attachment: fixed;
    background-position: center;
    width: 100%;
    background-color: black;
}
.mainheader {
    display: flex;
    height: auto;
    color: #EE96A2;
    position: absolute;
    text-wrap: wrap;
    justify-content: left;
    margin-top: 220px;
    margin-right: 400px;
    margin-left: 60px;
    color: white;
    font-family: Archivo Black;
    animation-name: changeColor;
    animation-duration: 6s;
    animation-iteration-count: infinite;
}
@keyframes changeColor {
    0% {
      color: white;
      opacity: 1;
    }
    60% {
      color: #EE96A2;
      filter: blur(2px);
      opacity: 1;
    }
    100% {
      color: white;
      opacity: 1;
    }
  }

/* blog page css starts here ------------------------------------ ------------------------------------  */

.newHeaderBox {
    display: flex;
    flex-wrap: wrap;
    justify-content: left;
    padding: 0px;
    width: 100%;
    height: auto;
    border-top: .5px solid black;
    border-bottom: .5px solid black;
}

.newHeader {
    display: flex;
    width: auto;
    height: auto;
    margin-top: 10px;
    align-items: center;
    justify-content: left;
    margin-left: 40px;
    text-align: center;
    font-family: Archivo Black;
    letter-spacing: 5px;
    border-right: 1px solid black;
    border-left: 1px solid black;
}
.newHeader2 {
    display: flex;
    width: auto;
    height: auto;
    margin-top: 10px;
    align-items: center;
    justify-content: left;
    margin-left: 20px;
    text-align: center;
    font-family: Archivo Black;
    color: #fe7c7c;
    // border: 1px solid black;
    opacity: .5;
    // text-decoration: underline #ff8282;
}
.newestPost1 {
    display: block;
    width: 100%;
    min-width: 325px;
    max-width: 325px;
    margin-left: auto;
    margin-right: auto;
}
.newestPost1 img {
    height: auto;
    width: 100%;

}
.newestPost {
    display: block;
    width: 50%;
    margin-left: auto;
    margin-right: auto;
    min-width: 325px;
}
.newestPost *{
    display: flex;
    justify-content: left;
    flex-wrap: wrap;
    width: 100%;
}
.newestText {
    display: flex;
    justify-content: left;
    flex-wrap: wrap;
    width: 100%;
    margin-top: 10px;
    font-size: 16pt;
}
.newestDate {
    width: 100%;
    display: flex;
    color: #EE96A2;
    font-size: 10pt;
}
.newestPost img {
    width: 100%;
    height: auto;
    display: flex;
    box-shadow: 0 0 5px 0;
    
}
.blogHalves {
    width: 100%;
    height: auto;
    display: flex;
    flex-wrap: wrap;
    gap: 50px;
    padding: 20px;
    margin-left: auto;
    margin-right: auto;
}
.blogNavBar {
    margin-top: 0px;
    display: flex;
    height: 50px;
    width: 50%;
    gap: 50px;
    flex-direction: row;
}
.mostRecentText {
    width: 100%; 
    display: flex; 
    justify-content: left; 
    text-align: center;
    align-items: center;
    margin-top: 10px;
    text-decoration: underline .5px #EE96A2;
    color:#000000;
    // border: 1px solid black;
    font-family: Montserrat;
    font-size: 10pt;
    border-radius: 3px;
    letter-spacing: 2px;
}
.otherNew {
    display: flex;
    justify-content: space-evenly;
    flex-wrap: wrap;
    width: 50%;
    height: auto;
    gap: 50px;
    padding: 0px;
    margin: 20px;
    border-left: 1px solid black;
}
.otherNew button {
    display: flex;
    flex-direction: column;
    flex-wrap: wrap;
    height: 30px;
    font-family: Archivo Black;
    border-radius: 4px;
    border: none;
}
.otherNew button:hover {
    color: #ff8282;
}
#blogborder {
    display: flex;
    width: 55%;
    border-top: 1px solid black;
    justify-content: center;
    margin-left: auto;
    margin-right: auto;
    margin-top: -30px;
    opacity: .5;
}
#blogborder2 {
    display: flex;
    width: 55%;
    border-top: 1px solid black;
    justify-content: center;
    margin-left: auto;
    margin-right: auto;
    margin-top: 10px;
    opacity: .5;
}
.blogintrocontainer {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    align-items: center;
    width: 100%;
    padding:4%;
}
.blogintro {
    flex-direction: row;
    flex-basis: 450px;
}
.blogintro > p {
    color: #fe7c7c;
    width: 100%;
    text-align: justify;
    padding: 20px;
}
.blogintro2 {
    flex-direction: row;
    flex-basis: 450px;
}
.blogintro2 > img {
    display: flex;
    justify-content: center;
    align-items: center;
    width: 100%;
    height: auto;
    padding: 20px;
} 
.qoute {
    display: flex;
    width: 100%;
}
.quote > h1 {
    display: flex;
    font-family: Montserrat;
    justify-content: center;
    text-align: center;
    margin-top: 120px;
}
.quote2 {
    display: inline-flex;
    justify-content: center;
    align-items: center;
    width: 100%;
    height: auto;
    padding: 10px;
    gap: 20px;
    opacity: .6;
    margin-bottom: 5px;
}
.quote2 button {
    padding: 5px 10px;
    flex: 0;
    display: flex;
    font-family: Montserrat;
    margin-top: 0px;
    text-wrap: nowrap;
    color: #a4a4a4;
