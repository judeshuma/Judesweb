$light: white;
$primary: black;
$dark: black;

/* Global Settings  ------------------------------------ ------------------------------------   */

img {
    height: 35px;
    width: 35px;
}
.navbar-nav {
    display: flex;
    margin-left: 5px;
    letter-spacing: 7px;
    font-size: 15px;
    font-family: 'Montserrat', sans-serif;
}
.nav-link {
    text-wrap: nowrap;
}
.nav-link a:active {
    background-color: rgb(0, 0, 0);
    color:#fe7c7c;
}
.navbar-nav a:hover {
    color:#ff8282;
    background-color: #000000;
}
.containerheader {
    display: flex;
    justify-content: center;
    text-align: center;
    font-family: Archivo Black;
    height: auto;
    width: 100%;
    color: rgb(0, 0, 0);
    border-top: .2px solid black;
}
#judesWeb {
    display:flex;  
    color: white;
    width: 100%; 
    overflow: hidden;
    margin-top: -150px;
    font-family: Archivo Black;
}
#blurb {
    display:flex;  
    color: white;
    width: 100%; 
    overflow: hidden;
    margin-top: 4%;
    font-family: Archivo Black;
    font-size: 15pt;
    text-decoration: underline #fe7c7c;
    justify-content: center;
    text-align: center;
    letter-spacing: 20px;

}
.recentPosts {
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
    height: auto;
    width: 100%;
    gap: 50px;

}
.recentPostsInner {
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
    height: auto;
    width: 100%;
    margin: 10px;
    gap: 20px;

}
